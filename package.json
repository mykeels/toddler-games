{
  "name": "toddler-games",
  "private": true,
  "version": "0.0.9",
  "type": "module",
  "scripts": {
    "predeploy": "npm version patch && npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "vite",
    "start": "vite",
    "build": "tsc -b && npx cross-env ASSET_URL=/toddler-games/ vite build --base=/toddler-games/",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@tanstack/react-router": "^1.57.15",
    "clsx": "^2.1.1",
    "gh-pages": "^6.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@tanstack/router-devtools": "^1.57.15",
    "@tanstack/router-plugin": "^1.57.15",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "gh-pages-deploy": "^0.5.1",
    "globals": "^15.9.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.1",
    "vite-plugin-pwa": "^0.20.5"
  },
  "gh-pages-deploy": {
    "staticpath": "dist",
    "prep": [
      "build"
    ],
    "commit": "Deploy to gh-pages",
    "post": [],
    "noprompt": false
  }
}
